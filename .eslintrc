{
  "env": {
    "node": true,
    "builtin": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [
      2,
      "last"
    ],
    "curly": [
      2,
      "all"
    ],
    "dot-notation": [
      2,
      {
        "allowKeywords": false
      }
    ],
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "indent": ["error", 4],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": "off",
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-trailing-spaces": "error",
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-with": 2,
    "quotes": [
      2,
      "double"
    ],
    "semi": [
      0,
      "always"
    ],
    "space-infix-ops": "error",
    "space-unary-ops": [
      2, {
        "words": true,
        "nonwords": false
    }],
    "strict": [2, "global"],
    "valid-typeof": 2,
    "wrap-iife": [
      2,
      "inside"
    ]
  }
}